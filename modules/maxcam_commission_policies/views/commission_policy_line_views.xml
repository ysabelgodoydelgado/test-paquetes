<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data>
        <record id="commission_policy_lines_tree_view" model="ir.ui.view">
            <field name="name">commission.policy.lines.tree.view</field>
            <field name="model">commission.policy.line</field>
            <field name="arch" type="xml">
                <tree editable="bottom">
                    <field name="date_from" />
                    <field name="date_until"/>
                    <field name="commission"/>
                    <field name="percentage_report"/>
                    <field name="not_applied"/>
                </tree>
            </field>
        </record>
    </data>

</odoo>
